{
  "name": "ts-admin-components",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:dev": "vue-tsc && vite build --watch",
    "build:pub": "npm run build && npm link && npm publish",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "base64toblob": "^0.0.2",
    "primeicons": "^6.0.1",
    "primevue": "3.29.2",
    "rfs": "^10.0.0",
    "sass": "^1.62.1",
    "vee-validate": "^4.9.5",
    "vue": "^3.2.45",
    "vue-advanced-cropper": "^2.8.8",
    "vue-router": "^4.1.6",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.2.1",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "2.8.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.0.24"
  },
  "description": "Global reusable components for micro frontend",
  "main": "./dist/ts-admin-components.umd.js",
  "module": "./dist/ts-admin-components.es.js",
  "exports": {
    ".": {
      "import": "./dist/ts-admin-components.es.js",
      "require": "./dist/ts-admin-components.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "./dist/main.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rndts/ts-admin-components.git"
  },
  "author": "rndts",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rndts/ts-admin-components/issues"
  },
  "homepage": "https://github.com/rndts/ts-admin-components#readme"
}
